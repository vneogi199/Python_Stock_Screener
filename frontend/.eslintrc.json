{
	"extends": ["airbnb", "prettier"],
	"plugins": ["prettier"],
	"rules": {
		"prettier/prettier": "error",
		"no-unused-vars": "warn",
		"no-console": "warn",
		"func-names": "off",
		"no-tabs": "off",
		"react/jsx-filename-extension": "off",
		"react/react-in-jsx-scope": "off",
		"spaced-comment": "off",
		"comma-dangle": "off"
	},
	"ignorePatterns": ["index", "reportWebVitals.js"],
	"env": {
		"browser": true
	}
}
